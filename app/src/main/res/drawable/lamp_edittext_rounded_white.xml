<?xml version="1.0" encoding="UTF-8"?>
<!-- 自定义圆输入框（获得焦点） -->
<shape xmlns:android="http://schemas.android.com/apk/res/android" >
    
	<!-- 输入框内的颜色 -->
    <solid android:color="#FFFFFF" />
	<!-- 半径 -->
    <corners android:radius="2dip" />
	<!-- 输入框边的颜色 -->
    <stroke
        android:width="0.5dip"
        android:color="#DDDDDD" />

</shape>